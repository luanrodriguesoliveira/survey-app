    "@typescript-eslint/parser": "^4.17.0",
    "eslint": "^7.21.0",
    "ts-node": "3.3.0",
    "ts-node-dev": "^1.1.6",
    "tsc": "^1.20150623.0",
    "typescript": "3.3.3333"
  },
  "dependencies": {
    "@types/argon2": "^0.15.0",
    "argon2": "^0.27.1",
    "module-alias": "^2.2.2",
    "mysql": "^2.14.1",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.10",
    "typeorm": "^0.2.31"
